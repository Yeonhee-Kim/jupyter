   "source": [
    "# 문제 1\n",
    "\n",
    "S = input()\n",
    "\n",
    "S = S.upper() # 대문자 변환\n",
    "\n",
    "output = \"\"\n",
    "\n",
    "i = 0\n",
    "while i < len(S):\n",
    "    output += S\n",
    "    if i < len(S) - 1:\n",
    "        output += \",\"\n",
    "    i += 1\n",
    "\n",
    "print(output)"
   ]
  }
